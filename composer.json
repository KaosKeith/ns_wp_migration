{
    "name": "nitsan/ns-wp-migration",
    "type": "typo3-cms-extension",
    "description": "Plug-n-play TYPO3 extension to migrate wordpress post and page data to your TYPO3 site. This extension includes features like Migrate post to EXT:news or EXT:blog, migrate categories, migrate tags, migrate media files etc.",
    "authors": [
        {
            "name": "Navdeepsinh Jethwa",
            "role": "Developer",
            "email": "sanjay@nitsan.in",
            "homepage": "https://nitsantech.com/"
        }
    ],
    "license": "GPL-2.0-or-later",
    "keywords": [
        "TYPO3 CMS",
        "Migration",
        "Blog",
        "NITSAN",
        "News",
        "Wordpress"
    ],
    "require": {
        "typo3/cms-core": "^11.5.24 || ^12.4.2",
        "ezyang/htmlpurifier": "^4.14",
        "t3g/blog": "^12.0.0",
        "georgringer/news": "^11.4",
        "mediadreams/md_news_author":"^7.0",
        "nitsan/ns-basetheme": "^11.4 || ^12.1"
    },
    "require-dev": {
        "typo3/testing-framework": "^6.9.0"
    },
    "autoload": {
        "psr-4": {
            "NITSAN\\NsWpMigration\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NITSAN\\NsWpMigration\\Tests\\": "Tests"
        }
    },
    "replace": {
        "typo3-ter/ns-wp-migration": "self.version"
    },
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin"
    },
    "scripts": {
        "post-autoload-dump": [
            "TYPO3\\TestingFramework\\Composer\\ExtensionTestEnvironment::prepare"
        ]
    },
    "extra": {
        "typo3/cms": {
            "cms-package-dir": "{$vendor-dir}/typo3/cms",
            "web-dir": ".Build/public",
            "extension-key": "ns_wp_migration"
        }
    }
}